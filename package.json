{
  "name": "juanstate",
  "version": "1.2.0",
  "description": "Simple state handler for react",
  "author": "jkierem",
  "license": "MIT",
  "main": "juanstate/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jkierem/JuanState"
  },
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\"  \"npm run docs\"",
    "lib": "babel src/juanstate -d juanstate --copy-files",
    "lib:watch": "babel src/juanstate -w -d juanstate --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production"
  },
  "peerDependencies": {
    "react": "~16.4.1",
    "react-dom": "~16.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
